{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development  nodemon",
    "prestart:prod": "tsc -p tsconfig.json",
    "start:prod": "node dist/server.js",
    "dist": "npm run prestart:prod",
    "debug": "node --nolazy --debug-brk=5858 ./index.js",
    "test:unit": "cross-env NODE_ENV=development jest --coverage -c jest-config.json",
    "test:e2e": "cross-env NODE_ENV=development jest --coverage -c jest-e2e-config.json",
    "test": "npm run unit && npm run e2e"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/swagger": "^1.2.0",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.5.8",
    "body-parser": "^1.18.2",
    "class-validator": "^0.8.5",
    "connect-history-api-fallback": "^1.5.0",
    "ejs": "^2.5.8",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "pg": "^7.4.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "ts-jest": "^22.4.4",
    "typeorm": "^0.1.20",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chalk": "^0.4.31",
    "@types/express": "^4.0.39",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "chalk": "^2.1.0",
    "cross-env": "^5.1.4",
    "envify": "^4.1.0",
    "jest": "^22.4.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0"
  }
}
